{
  "npmScope": "alpha-components",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "docs": { "tags": ["type:app", "target:react"] },
    "dev-react": { "tags": ["type:app", "target:react"] },
    "dev-angular": { "tags": ["type:app", "target:angular"] },
    "react-button": { "tags": ["type:component", "target:react"] },
    "angular-button": { "tags": ["type:component", "target:angular"] },
    "react-select": { "tags": ["type:component", "target:react"] }
  }
}
