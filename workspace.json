{
  "version": 2,
  "projects": {
    "docs": {
      "root": "apps/docs",
      "sourceRoot": "apps/docs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/docs",
            "index": "apps/docs/src/index.html",
            "main": "apps/docs/src/main.tsx",
            "polyfills": "apps/docs/src/polyfills.ts",
            "tsConfig": "apps/docs/tsconfig.app.json",
            "assets": [
              "apps/docs/src/assets"
            ],
            "styles": [
              "apps/docs/src/styles/index.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/docs/src/environments/environment.ts",
                  "with": "apps/docs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "docs:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "docs:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/docs/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/docs"
          ],
          "options": {
            "jestConfig": "apps/docs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
