    "dev-angular": {
      "projectType": "application",
      "root": "apps/dev-angular",
      "sourceRoot": "apps/dev-angular/src",
      "prefix": "alpha-components",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
